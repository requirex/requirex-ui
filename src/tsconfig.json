{
	"compileOnSave": true,
	"compilerOptions": {
		"declaration": true,
		"jsx": "react",
		"jsxFactory": "h",
		"lib": [ "dom", "es5", "es2015.collection", "es2015.promise" ],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"outDir": "../dist",
		"removeComments": false,
		"sourceMap": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"target": "es5"
	},
	"files": [
		"index.tsx"
	]
}
